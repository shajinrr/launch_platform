[stge_app_servers]
stge_app ansible_host=172.19.0.7 ansible_user=centos ansible_ssh_private_key_file=..//..//..//jenkins_data/.ssh/id_rsa ansible_ssh_common_args='-o StrictHostKeyChecking=no' ansible_become=yes

[stge_web_servers]
stge_web ansible_host=172.19.0.8 ansible_user=ubuntu ansible_ssh_private_key_file=..//..//..//jenkins_data/.ssh/id_rsa ansible_ssh_common_args='-o StrictHostKeyChecking=no' ansible_become=yes
